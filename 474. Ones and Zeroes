class Solution {
    public int findMaxForm(String[] strs, int M, int N) {
        int[][] dp = new int[M + 1][N + 1];
        
        for (String s : strs) {
            int mCnt = 0, nCnt = 0;
            for (char c : s.toCharArray()) {
                if (c == '0') mCnt++; 
                else nCnt++;
            }
            
            for (int m = M; m >= mCnt; m--) {
                for (int n = N; n >= nCnt; n--) {
                    dp[m][n] = Math.max(dp[m][n], 1 + dp[m - mCnt][n - nCnt]);
                }
            }
        }
        
        return dp[M][N];
    }
}
